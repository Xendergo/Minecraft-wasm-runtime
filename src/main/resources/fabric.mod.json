{
  "schemaVersion": 1,
  "id": "wasmruntime",
  "version": "${version}",

  "name": "Minecraft wasm runtime",
  "description": "Make custom plugins for minecraft using any language that can compile to wasm",
  "authors": [
    "Xendergo"
  ],
  "contact": {
    "homepage": "https://github.com/Xendergo/Minecraft-wasm-runtime",
    "sources": "https://github.com/Xendergo/Minecraft-wasm-runtime"
  },

  "license": "CC0-1.0",
  "icon": "assets/wasm_runtime/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "wasmruntime.WasmRuntime"
    ]
  },

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}